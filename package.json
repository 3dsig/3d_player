{
  "name": "3d_player",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --config webpack.config.babel.js",
    "release": "webpack --p --config webpack.config.babel.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "deploy": "",
    "lint": "eslint ./src",
    "build-examples": "webpack --config examples/webpack.config.babel.js --progress",
    "start": "webpack-dev-server --config examples/webpack.config.babel.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-loader-advanced": "^1.7.1",
    "react-loading": "^1.0.3",
    "react-redux-i18n": "^1.9.1",
    "react-tooltip": "^3.4.0",
    "wavesurfer.js": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.0.10",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
